{
  "name": "Vaxowave",
  "version": "0.1",
  "description": "Vaxowave extension to help with Jupyter Notebooks",
  "permissions": [
    "*://*/*.ipynb",
    "activeTab",
    "declarativeContent",
    "storage",
    "debugger",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/128.png"
  },
  "web_accessible_resources": [
    "css/style.css"
  ],
  "content_scripts": [
    {
      "css": [
        "css/style.css",
        "css/codemirror-monokai.css"
      ],
      "matches": [
        "*://*/*.ipynb"
      ]
    }
  ],
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "manifest_version": 2
}